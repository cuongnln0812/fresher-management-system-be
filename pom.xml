<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 https://maven.apache.org/xsd/maven-4.0.0.xsd">
    <modelVersion>4.0.0</modelVersion>
    <parent>
        <groupId>org.springframework.boot</groupId>
        <artifactId>spring-boot-starter-parent</artifactId>
        <version>3.2.2</version>
        <relativePath/> <!-- lookup parent from repository -->
    </parent>
    <groupId>com.example</groupId>
    <artifactId>phase1_FAMS</artifactId>
    <version>0.0.1-SNAPSHOT</version>
    <name>phase1_FAMS</name>
    <description>phase1_FAMS</description>
    <properties>
        <java.version>17</java.version>

        <sonar.host.url>http://anataarisa.hopto.org:2000</sonar.host.url>
        <sonar.token>sqp_1b2011f04bb2059306a94d84f466e38ce31b2524</sonar.token>
        <sonar.projectName>fams-sonarqube-group3</sonar.projectName>
        <sonar.projectKey>fams-sonarqube-group3</sonar.projectKey>
        <sonar.qualitygate.wait>true</sonar.qualitygate.wait>

        <!-- JaCoCo Properties -->
        <jacoco.version>0.8.11</jacoco.version>
        <sonar.java.coveragePlugin>jacoco</sonar.java.coveragePlugin>
        <sonar.dynamicAnalysis>reuseReports</sonar.dynamicAnalysis>
        <sonar.coverage.jacoco.xmlReportPaths>${project.basedir}/target/site/jacoco/jacoco.xml</sonar.coverage.jacoco.xmlReportPaths>
        <sonar.language>java</sonar.language>
    </properties>
    <dependencies>
        <dependency>
            <groupId>org.springframework.boot</groupId>
            <artifactId>spring-boot-starter-data-jpa</artifactId>
